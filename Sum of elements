int main() {
    
    int m,n;
    scanf("%d%d",&m,&n);
     int a[m][n];
     int i,j,s=0;
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++)
       s=s+a[i][j];
      }
      printf("%d",s);

      printf("\n");
    }
